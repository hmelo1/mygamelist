<div class="col-md-12">
  <div class ="panel panel-default">
    <div class="panel-heading">
      <h2>Your Game List</h2>
    </div>
    <div class="panel-body">
      <% @list.each do |game| %>
      <div class="container">
        <div class="col-md-3">
          <%= game.game.title %>
        </div>
        <div class="col-md-3">
        </div>
        <div class="col-md-3">
        </div>
        <div class="col-md-3">
          <!-- button to remove in AJAX -->
          <%= link_to root_url + "gamelists/" + game.id.to_s, method: :delete, data: {confirm: "Are you sure?"} do%>
            <i class="fa fa-trash-o" aria-hidden="true"></i>
          <% end %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
