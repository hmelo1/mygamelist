<div class="col-md-12">
  <div class ="panel panel-default">
    <div class="panel-heading">
      <h2>Your Game List</h2>
    </div>
    <div class="panel-body">
      <% @list.each do |game| %>
      <div class="container">
        <div class="col-md-3">
          <%= game.game.title %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
