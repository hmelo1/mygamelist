<% @list.each do |game| %>
  <div class="container">
    <div class="col-md-3">
      <%= game.game.title %>
    </div>
    <div class="col-md-3">
    </div>
    <div class="col-md-3">
    </div>
    <div class="col-md-3">
      <%= link_to root_url + "gamelists/" + game.id.to_s, method: :delete, data: {confirm: "Are you sure?"} do%>
        <i class="fa fa-trash-o" aria-hidden="true"></i>
      <% end %>
    </div>
  </div>
<% end %>
